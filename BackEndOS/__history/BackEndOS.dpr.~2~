program BackEndOS;
{$APPTYPE GUI}

uses
  Vcl.Forms,
  Web.WebReq,
  IdHTTPWebBrokerBridge,
  FormUOS in 'FormUOS.pas' {Form1},
  ServerMethodsUOS in 'ServerMethodsUOS.pas' {ServerMethods1: TDataModule},
  ServerContainerUOS in 'ServerContainerUOS.pas' {ServerContainer1: TDataModule},
  WebModuleUOS in 'WebModuleUOS.pas' {WebModule1: TWebModule};

{$R *.res}

begin
  if WebRequestHandler <> nil then
    WebRequestHandler.WebModuleClass := WebModuleClass;
  Application.Initialize;
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
